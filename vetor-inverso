/******************************************************************************
Nicoy Ferreira
ex 5
crie um programa que leia 6 valores inteiros. inteiros
em seguida, mostre os valores em ordem inversa 
*******************************************************************************/
#include <stdio.h>

int main()
{
    int valores[5];
    int i;
   
   
   for(i=0;i<6;i++) {
       printf("Digite o %d numeros: ", i+1) ;
   scanf("%d", &valores[i]);
   }
   
   
   printf("Valores em inverso: ");
   for(i=5;i>=0;i--) {
      printf("\nv[%d]",valores[i]);
       
   }

    return 0;
    
    
}

